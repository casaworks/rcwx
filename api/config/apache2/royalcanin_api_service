<VirtualHost *:80>
    ServerName www.geekernel.com
    WSGIDaemonProcess api_service threads=5
    WSGIScriptAlias / /var/www/royalcanin/current/api/api_service.wsgi
    <Directory /var/www/royalcanin/current/api/>
        WSGIProcessGroup api_service 
        WSGIApplicationGroup %{GLOBAL}
        Order deny,allow
        Allow from all
    </Directory>
</VirtualHost>
